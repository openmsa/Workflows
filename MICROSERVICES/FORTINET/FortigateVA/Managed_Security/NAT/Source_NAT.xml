  </variables>  <example>
    <content>UBI-SITONECELL-MASTE~001 # show full-configuration firewall ippool 
config firewall ippool
    edit "testPSM"
        set type overload
        set startip 10.2.3.3
        set endip 10.2.3.3
        set arp-reply enable
        set arp-intf ''
        set comments "test"
