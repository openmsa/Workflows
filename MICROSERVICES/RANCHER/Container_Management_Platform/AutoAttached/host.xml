  <command name="IMPORT">
    <operation>GET</operation>
    <xpath>v2-beta/projects/{$PROJECT_ID}/hosts</xpath>
    <parser>
      <section>
        <xpath>//root/data/row</xpath>
      </section>
      <lines>
        <line>
          <xpath>id</xpath>
          <regexp>@&lt;id&gt;(?&lt;object_id&gt;.*)&lt;/id&gt;@</regexp>
        </line>
        <line>
          <xpath>name</xpath>
          <regexp>@&lt;name&gt;(?&lt;name&gt;.*)&lt;/name&gt;@</regexp>
        </line>
        <line>
          <xpath>state</xpath>
          <regexp>@&lt;state&gt;(?&lt;state&gt;.*)&lt;/state&gt;@</regexp>
        </line>
        
        
        <line>
          <xpath>uuid</xpath>
          <regexp>@&lt;uuid&gt;(?&lt;uuid&gt;.*)&lt;/uuid&gt;@</regexp>
        </line>
      </lines>
    </parser>
    <post_template>{assign_object_variable var="object_display_var" value="{$params.name} - {$params.object_id} - {$params.state}"}</post_template>
  </command>
