  <command name="IMPORT">
    <operation>GET</operation>
    <xpath>v2-beta/projects/{$PROJECT_ID}/containers</xpath>
    <parser>
      <section>
        <xpath>//root/data/row</xpath>
      </section>
      <lines>
        <line>
          <xpath>id</xpath>
          <regexp>@&lt;id&gt;(?&lt;object_id&gt;.*)&lt;/id&gt;@</regexp>
        </line>
        <line>
          <xpath>name</xpath>
          <regexp>@&lt;name&gt;(?&lt;name&gt;.*)&lt;/name&gt;@</regexp>
        </line>
        <line>
          <xpath>state</xpath>
          <regexp>@&lt;state&gt;(?&lt;state&gt;.*)&lt;/state&gt;@</regexp>
        </line>
        
        <line>
          <xpath>uuid</xpath>
          <regexp>@&lt;uuid&gt;(?&lt;uuid&gt;.*)&lt;/uuid&gt;@</regexp>
        </line>
        <line>
          <xpath>imageUuid</xpath>
          <regexp>@&lt;imageUuid&gt;(?&lt;image_uuid&gt;.*)&lt;/imageUuid&gt;@</regexp>
        </line>
        <line>
          <xpath>hostname</xpath>
          <regexp>@&lt;hostname&gt;(?&lt;hostname&gt;.*)&lt;/hostname&gt;@</regexp>
        </line>
        <line>
          <xpath>primaryIpAddress</xpath>
          <regexp>@&lt;primaryIpAddress&gt;(?&lt;primary_ip_address&gt;.*)&lt;/primaryIpAddress&gt;@</regexp>
        </line>
        <line>
          <array name="ports">
            <xpath>ports</xpath>
            <lines>
              <line>
                <xpath>//row</xpath>
                <regexp>@&lt;row&gt;(?&lt;port&gt;.*)&lt;/row&gt;@</regexp>
              </line>
            </lines>
          </array>
        </line>
      </lines>
    </parser>
    <post_template/>
  </command>
<command name="CREATE">
    <rest>POST
</rest>
    <xpath>v2-beta/projects/{$PROJECT_ID}/containers
</xpath>
    <operation>{literal}{{/literal}
"name":"{$params.name}",
"imageUuid":"{$params.image_uuid}"
{if !empty($params.ports)}
,"ports":[
{foreach $params.ports as $port}
{if $port@iteration &gt; 1},{/if}
"{$port.host}:{$port.container}"
{/foreach}
]
{/if}
{if !empty($params.hostname)}
,"hostname":"{$params.hostname}"
{/if}
{literal}}{/literal}
</operation>
  </command>
