<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ObjectDefinition>  <information>
    <dynamic>false</dynamic>
    <icon>/images/eclipseIcons/page_obj.gif</icon>
    <configType>xml</configType>
    <visible>false</visible>
    <importIfMandatoryPresent>false</importIfMandatoryPresent>
    <sortvariable>params._order</sortvariable>
    <sortascending>true</sortascending>
    <sortnumerical>true</sortnumerical>
    <sortauto>true</sortauto>
    <order>10</order>
    <description/>
    <name>Zones</name>
    <defaultDisplay>false</defaultDisplay>
    <group>Objects|Network</group>
  </information>  <variables frozen="0">
    <variable numberOfRowMax="3000" displayName="Zone Name" displayOrder="0" maxLength="150" name="params.object_id" type="String"/>
  </variables>  <example>
    <content>&lt;zone&gt;
            &lt;entry name="TEST2_R1_vFW"&gt;
              &lt;network&gt;
                &lt;layer3&gt;
                  &lt;member&gt;ae1.1&lt;/member&gt;
                &lt;/layer3&gt;
              &lt;/network&gt;
            &lt;/entry&gt;
            &lt;entry name="TEST2_R1_VM"&gt;
              &lt;network&gt;
                &lt;layer3&gt;
                  &lt;member&gt;ae2.1&lt;/member&gt;
                &lt;/layer3&gt;
              &lt;/network&gt;
            &lt;/entry&gt;
          &lt;/zone&gt;
</content>
  </example>
  <command name="IMPORT">
    <operation>type=config&amp;action=show</operation>
    <xpath>/config/devices/entry[@name='localhost.localdomain']/vsys/entry[@name='{$SD-&gt;SD_HOSTNAME}']/zone</xpath>
    <parser>
      <section>
        <xpath>//zone/entry</xpath>
      </section>
      <lines>
        <line>
          <xpath>//entry/@name</xpath>
          <regexp>@name="(?&lt;object_id&gt;.*)"@</regexp>
        </line>
      </lines>
    </parser>
  </command>
</ObjectDefinition>
